{
  "name": "USOM Web Blocker",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "USOM Web Blocker",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "index.js"
  },
  "permissions": [
    "webRequest",
    "declarativeNetRequest",
    "unlimitedStorage",
    "storage"
  ],
  "host_permissions": [
    "*://*/"
  ]
}